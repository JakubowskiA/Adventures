<h1>Edit Adventure</h1>
<br>
<br>
<%=form_for @adventure do |f|%>
<%=f.label :name%>
<br>
<%=f.text_field :name%>
<br>
<br>
<%=f.label :memory%>
<br>
<%=f.text_area :memory%>
<br>
<br>
<%=f.label :year%>
<br>
<%=f.number_field :year%>
<br>
<br>
<%=f.label :place_id%>
<%=collection_select(:adventure, :place_id, Place.all, :id, :name, prompt: true)%>
<br>
<br>
<%=f.submit%>
<% end %>